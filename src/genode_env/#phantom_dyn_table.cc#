#include <cstdint>
#include <phantom_env.h>
#include <base/heap.h>

extern "C" {
  class DynamicTable {
  public:
    static const uint16_t PAGE_SIZE = 4096;
    
    DynamicTable() {
      
    }
    ~DynamicTable() {}

  private:
    struct Block {
      addr_t       *address;
      struct Block *next;
    };

    addr_t *start;

    
  };
}